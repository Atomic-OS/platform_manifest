<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch="https://android-review.googlesource.com/" />

  <remote name="github"
           fetch="https://github.com/" />

  <remote name="gitlab"
          fetch="https://gitlab.com" />

  <remote  name="gzosp"
           fetch="https://github.com/GZOSP"
           revision="8.1" />

  <remote name="aosssh"
          fetch="ssh://github.com/Atomic-OS/" />

  <remote  name="aos"
           fetch="https://github.com/Atomic-OS"
           revision="oreo" />

  <remote  name="omni"
	   fetch="https://github.com/omnirom"
	   revision="android-8.1" />

  <default revision="refs/tags/android-8.1.0_r14"
           remote="aosp"
           sync-j="4" />

  <!-- AOSP -->
  <include name="aosp.xml" />

  <!-- CAF -->
  <include name="caf.xml" />

  <!-- GZOSP -->
  <include name="gzosp.xml" />

  <!-- Atomic-OS -->
  <include name="aos.xml" />

</manifest>
